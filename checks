SELECT * INTO SQLCRIS_User.dbo.Afernandes_ADDRESS


FROM 
(


SELECT *, 

 ROW_NUMBER  () 
OVER (PARTITION BY 			ReferralBrcid
							ORDER BY DIFF, 
							DIFF, 
							DIFF
) as ranked_address 


FROM

(
SELECT ReferralBrcid, LSOA11, Start_Date as addressstartdate, CN_Doc_ID as addcndocid, BrcId as addbrcid 
,ABS(DATEDIFF(DD, Referral_accepted_date, Start_Date)) as DIFF

FROM  SQLCRIS_User.dbo.Afernandes_FC10102017 AS FC

LEFT JOIN

SQLCRIS.dbo.Address AS ADDRESS

ON ADDRESS.BrcId = FC.ReferralBrcid
)TableB

)TableC

where ranked_address = 1






















(
SELECT * FROM SQLCRIS_User.dbo.Afernandes_ADDRESS AS FCaddress

left join 

SQLCRISImport.dbo.ons_2011_imd

on 
SQLCRISImport.dbo.ons_2011_imd.LSOA11CD = FCaddress.LSOA11
)




select * from dbo.Afernandes_FC10102017

left join

(
SELECT * FROM SQLCRIS_User.dbo.Afernandes_ADDRESS AS FCaddress

left join 

SQLCRISImport.dbo.ons_2011_imd

on 
SQLCRISImport.dbo.ons_2011_imd.LSOA11CD = FCaddress.LSOA11
)TableAddress


on TableAddress.R



